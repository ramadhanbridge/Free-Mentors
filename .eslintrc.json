{
    "env": {
        "mocha": true,
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "camelcase":"off",
        "eqeqeq":"off"
    }
}